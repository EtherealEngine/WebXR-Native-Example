    "watch": "cross-env NODE_ENV=development vite --port 3001 --hostname 127.0.0.1",
    "validate": "yarn run lint && yarn run test",
    "lint": "eslint \"**/*.tsx\"",
    "lint-fix": "eslint --fix --quiet \"*.ts\" \"**/*.ts\" \"**/*.tsx\"",
